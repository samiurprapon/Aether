{
	"name": "@samiurprapon/aether",
	"version": "2.1.0",
	"description": "Ravamp previous apis to much better technologies",
	"main": "src/index.ts",
	"author": "Samiur Prapon",
	"license": "MIT",
	"private": true,
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"prebuild": "rimraf ./dist",
		"build": "tsc && tsc-alias",
		"start": "node dist/src/index.js",
		"dev": "nodemon",
		"lint": "eslint src/**/*.ts --fix --ext .ts",
		"format": "prettier --write .",
		"migrate:generate": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:generate -d src/providers/index",
		"migrate:app": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/providers/postgres/Provider.ts",
		"revert:app": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/providers/postgres/Provider.ts",
		"devider": "echo \"----\"",
		"migrate:log": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/providers/mysql/Provider.ts",
		"revert:log": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/providers/mysql/Provider.ts"
	},
	"devDependencies": {
		"@esbuild-kit/esm-loader": "^2.6.5",
		"@types/bcrypt": "^5.0.2",
		"@types/compression": "^1.7.5",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/hpp": "^0.2.6",
		"@types/jest": "^29.5.12",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/morgan": "^1.9.9",
		"@types/multer": "^1.4.11",
		"@types/node": "^20.12.14",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.6",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^7.11.0",
		"@typescript-eslint/parser": "^7.11.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"jest": "^29.7.0",
		"nodemon": "^3.1.2",
		"prettier": "^3.2.5",
		"prisma": "^5.14.0",
		"ts-node": "^10.9.2",
		"tsc-alias": "^1.8.10",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"@prisma/client": "^5.14.0",
		"bcrypt": "^5.1.1",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.1.3",
		"eslint": "^8.42.0",
		"express": "^4.19.2",
		"helmet": "^7.1.0",
		"hpp": "^0.2.3",
		"ioredis": "^5.4.1",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.0",
		"multer": "^1.4.5-lts.1",
		"mysql2": "^3.10.0",
		"nanoid": "^5.0.7",
		"pg": "^8.11.5",
		"reflect-metadata": "^0.2.2",
		"rimraf": "^5.0.7",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.0",
		"typeorm": "^0.3.20",
		"uuid": "^9.0.1",
		"winston": "^3.13.0",
		"winston-daily-rotate-file": "^5.0.0"
	}
}
