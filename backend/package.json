{
	"name": "@samiurprapon/aether",
	"version": "2.1.1",
	"description": "Ravamp previous apis to much better technologies",
	"main": "src/index.ts",
	"author": "Samiur Prapon",
	"license": "MIT",
	"private": true,
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"prebuild": "rimraf ./dist",
		"build": "tsc && tsc-alias",
		"start": "cross-env NODE_ENV=production node dist/src/index.js",
		"dev": "cross-env NODE_ENV=development nodemon",
		"test": "cross-env NODE_ENV=test jest",
		"coverage": "cross-env NODE_ENV=test jest --coverage",
		"start:dev": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register ./src/index.ts --port",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"migrate:db": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/providers/postgres/DataProvider.ts",
		"revert:db": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/providers/postgres/DataProvider.ts",
		"migrate:logs": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/providers/logs/LogProvider.ts",
		"revert:logs": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/providers/logs/LogProvider.ts",
		"prepare": "husky",
		"postinstall": "husky",
		"commmitmsg": "npx commitlint -x @commitlint/config-conventional -E HUSKY_GIT_PARAMS"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.5.0",
		"@commitlint/config-conventional": "^19.5.0",
		"@eslint/js": "^9.9.0",
		"@types/bcrypt": "^5.0.2",
		"@types/compression": "^1.7.5",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/hpp": "^0.2.6",
		"@types/jest": "^29.5.13",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/morgan": "^1.9.9",
		"@types/multer": "^1.4.12",
		"@types/node": "^22.5.5",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.6",
		"@types/uuid": "^10.0.0",
		"cross-env": "^7.0.3",
		"eslint": "^9.10.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"husky": "^9.1.6",
		"lint-staged": "^15.2.10",
		"jest": "^29.7.0",
		"nodemon": "^3.1.7",
		"prettier": "^3.3.3",
		"ts-node": "^10.9.2",
		"tsc-alias": "^1.8.10",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.5.4",
		"typescript-eslint": "^8.6.0"
	},
	"dependencies": {
		"bcrypt": "^5.1.1",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.1.3",
		"eslint": "^9.11.0",
		"express": "^4.21.0",
		"helmet": "^7.1.0",
		"hpp": "^0.2.3",
		"http-status-codes": "^2.3.0",
		"ioredis": "^5.4.1",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.0",
		"multer": "^1.4.5-lts.1",
		"mysql2": "^3.11.3",
		"nanoid": "^5.0.7",
		"pg": "^8.13.0",
		"reflect-metadata": "^0.2.2",
		"rimraf": "^6.0.1",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.1",
		"typeorm": "^0.3.20",
		"uuid": "^10.0.0",
		"winston": "^3.14.2",
		"winston-daily-rotate-file": "^5.0.0"
	},
	"lint-staged": {
		"**/*.{ts,json}": [
			"prettier --write",
			"eslint --fix"
		]
	}
}